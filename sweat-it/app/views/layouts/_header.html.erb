<div class="container" >
  <div class="row">
    <div class="col-12 pt-3 pb-3 mb-5 border-bottom">
    <div class="float-right">
        <% if user_signed_in? %>
            <%= link_to "Sign out", destroy_account_session_path, method: :delete, class: "btn btn-primary" %>
        <% else %>
        <%= link_to "Log in", new_user_session_path, class: "btn btn-outline-primary" %>
        <%= link_to "Sign up", new_user_registration_path, class: "btn btn-primary" %>
      </div>
      <p class="h3">Sweat It</p>
    </div>
  </div>
</div>